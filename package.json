{
  "name": "netflix",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "frontend": "vite",
    "backend": "cd server && npm start",
    "backend:dev": "cd server && npm run dev",
    "server": "cd server && npm start",
    "start:all": "concurrently \"npm run frontend\" \"npm run backend\" --names \"🎬Frontend,🚀Backend\" --prefix name --prefix-colors \"cyan,yellow\"",
    "dev:all": "concurrently \"npm run frontend\" \"npm run backend:dev\" --names \"🎬Frontend,🚀Backend\" --prefix name --prefix-colors \"cyan,yellow\"",
    "install:all": "npm install && cd server && npm install"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.1.2",
    "@tailwindcss/vite": "^4.0.6",
    "class-variance-authority": "^0.7.1",
    "firebase": "^11.3.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@reduxjs/toolkit": "^2.5.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "eslint": "^9.19.0",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.18",
    "globals": "^15.14.0",
    "postcss": "^8.5.2",
    "react-router-dom": "^7.1.5",
    "tailwindcss": "^4.0.6",
    "vite": "^6.1.0"
  }
}
